{
  "name": "pro-mern-stack",
  "version": "1.0.0",
  "description": "Sample project for learning MERN",
  "main": "app.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "nodemon -w dist -w static/index.html dist/server.js",
    "compile": "webpack",
    "watch": "webpack-dev-server --hot --inline",
    "compile-server": "babel server --presets es2015-node4 --out-dir dist --source-maps",
    "watch-server": "babel server --presets es2015-node4 --out-dir dist --source-maps --watch",
    "start-hook": "nodemon -w server server/start_hook.js",
    "dev-all": "npm start & npm run watch & npm run watch-server",
    "dev-all-hook": "npm run start-hook & npm run watch",
    "lint": "eslint --ext js,jsx src server webpack.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bwheel181/mern.git"
  },
  "author": "bryan wheeler",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/bwheel181/mern/issues"
  },
  "homepage": "https://github.com/bwheel181/mern#readme",
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "babel-register": "^6.26.0",
    "body-parser": "^1.18.2",
    "bootstrap": "^3.3.7",
    "classnames": "^2.2.5",
    "co": "^4.6.0",
    "express": "^4.16.2",
    "mongodb": "^2.2.33",
    "node-fetch": "^1.7.3",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-router-dom": "^4.2.2",
    "source-map-support": "^0.5.0",
    "whatwg-fetch": "^2.0.3"
